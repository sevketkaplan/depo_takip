<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>

        <Cell><Data ss:Type="String">Yakıt</Data></Cell>
        <Cell><Data ss:Type="String">MTV(Motorlu Taşıt Vergisi)</Data></Cell>
        <Cell><Data ss:Type="String">Bakım Ücreti</Data></Cell>
        <Cell><Data ss:Type="String">Araç Sigortası Ücreti</Data></Cell>
        <Cell><Data ss:Type="String">Arac Kasko Ücreti</Data></Cell>
        <Cell><Data ss:Type="String">Araç Plakası</Data></Cell>
        <Cell><Data ss:Type="String">Tarih</Data></Cell>
        
      </Row>
      <% Aracgideri.all.order("created_at DESC").each do |satilan| %>
      <Row>

        <Cell><Data ss:Type="String"><%=satilan.yakit%></Data></Cell>
        <Cell><Data ss:Type="String"><%=satilan.mtv%></Data></Cell>
        <Cell><Data ss:Type="String"><%=satilan.bakim%></Data></Cell>
        <Cell><Data ss:Type="String"><%=satilan.sigorta%></Data></Cell>
        <Cell><Data ss:Type="String"><%=satilan.kasko%></Data></Cell>
        <Cell><Data ss:Type="String"><%=satilan.plaka%></Data></Cell>
        <Cell><Data ss:Type="String"><%=satilan.created_at.strftime("%d-%m-%Y")%></Data></Cell>
      </Row>
      <% end %>
    </Table>
  </Worksheet>
</Workbook>